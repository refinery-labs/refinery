    "copy-to-clipboard": "3.2.0",
    "cytoscape": "^3.5.7",
    "debounce": "^1.2.0",
    "element-resize-detector": "^1.1.15",
    "fast-deep-equal": "^2.0.1",
    "file-saver": "2.0.2",
    "graphlib": "^2.1.7",
    "jszip": "^3.2.2",
    "lz-string": "^1.4.4",
    "modernizr": "^3.7.1",
    "moment": "^2.24.0",
    "monaco-editor": "^0.17.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "phone": "^2.3.14",
    "ramda": "^0.26.1",
    "register-service-worker": "^1.6.2",
