    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "fibers": "^3.1.1",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "sass": "^1.22.12",
    "sass-loader": "^7.3.1",
    "ts-jest": "^23.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
