{
  "name": "refinery-front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "vue-cli-service s3-deploy --mode production",
    "build-and-copy": "npm run build && ./build/replace-service-worker-paths.sh && ./build/copy-assets-from-dist-to-output-volume.sh && npm run deploy",
    "compile-validators": "./compile-typescript-validators.sh",
    "lint-fix": "vue-cli-service lint --fix",
    "modernizr": "modernizr -c modernizr-config.json -d node_modules/modernizr/modernizr.custom.js",
    "postinstall": "mkdir tmp-build; npm run modernizr; npm run compile-validators",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@types/cytoscape": "^3.8.3",
    "@types/debounce": "^1.2.0",
    "@types/file-saver": "^2.0.1",
    "@types/graphlib": "^2.1.5",
    "@types/jszip": "^3.1.6",
    "@types/lz-string": "^1.3.33",
    "@types/node": "^10.14.19",
    "@types/phone": "^1.0.3",
    "@types/ramda": "^0.26.9",
    "@types/split.js": "^1.3.2",
    "@types/stripe-v3": "^3.1.1",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.11.0",
    "@types/vue-markdown": "^2.2.1",
    "ajv": "^6.10.2",
    "animate.css": "^3.7.0",
