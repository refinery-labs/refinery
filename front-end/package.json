    "uuid": "^3.3.2",
    "validator": "^11.0.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-intercom": "1.0.14",
    "vue-jsx-hot-loader": "^1.4.1",
    "vue-markdown": "2.2.4",
    "vue-native-websocket": "^2.0.13",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.9.9",
    "vuex-persistedstate": "^2.5.4",
    "vuex-router-sync": "^5.0.0"
