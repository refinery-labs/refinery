{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "refinery-api-server",
      "memoryReservation": 800,
      "image": "134071937287.dkr.ecr.us-west-2.amazonaws.com/refinery-api-server-prod",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 7777
        },
        {
          "hostPort": 3333,
          "containerPort": 3333
        }
      ],
      "logConfiguration": {
      	"logDriver": "awslogs",
        "options": {
            "awslogs-group": "EB-Container-Output",
            "awslogs-region": "us-west-2"
        }
      }
    },
    {
      "name": "refinery-nginx-server",
      "memoryReservation": 160,
      "image": "134071937287.dkr.ecr.us-west-2.amazonaws.com/refinery-nginx-server",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}